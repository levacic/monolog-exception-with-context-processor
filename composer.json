{
    "name": "levacic/monolog-exception-with-context-processor",
    "description": "A Monolog processor to automatically extract context from suitable exceptions",
    "keywords": [
        "context",
        "errors",
        "exceptions",
        "logging",
        "monolog",
        "processor"
    ],
    "homepage": "https://github.com/levacic/monolog-exception-with-context-processor",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Milos Levacic",
            "email": "milos@levacic.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^8.1",
        "levacic/exception-with-context": "^1.0",
        "monolog/monolog": "^2.2|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Levacic\\Monolog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Levacic\\Monolog\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
